seq(1,20),seq(1,20),seq(1,20))
library(tidyverse)
s_plaque <- plaque %>% spread(key = "trt",value = "score")
View(s_plaque)
View(plaque)
ss <- s_plaque %>% filter(product=="P1")
t.test(ss$After,ss$Before,alternative = "l")
test_plaque <- function(prod) {
ss <- s_plaque %>% filter(product==prod)
t.test(ss$After,ss$Before,alternative = "l")
}
test_plaque("P1")
test_plaque("P2")
test_plaque("P3")
test_plaque <- function(prod) {
ss <- s_plaque %>% filter(product==prod)
t.test(ss$After,ss$Before,alternative = "l",paired = T)
}
test_plaque("P1")
test_plaque("P2")
test_plaque("P3")
